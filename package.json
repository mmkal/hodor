{
  "name": "hodor",
  "version": "0.0.1",
  "description": "Hodor. http://lisperator.net/pltut/parser/",
  "main": "hodor.hodor",
  "scripts": {
    "build": "tsc -p .",
    

    
    "watch": "chokidar \"**/*.ts\" \"**/*.hodor\" --command \"npm test\"",
    "_test": "ava js/test",
    "test": "npm run build && npm run _test",

    "watch-tap": "chokidar \"**/*.ts\" \"**/*.hodor\" --command \"npm run test-tap --silent\"",
    "_test-tap": "ava js/test --tap | tap-markdown",
    "test-tap": "echo pretap && npm run build && npm run _test-tap && posttap"
  },
  "keywords": [
    "hodor"
  ],
  "author": "mmkal",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "chokidar-cli": "^1.2.0",
    "glob": "^7.1.0"
  }
}
