{
  "name": "hodor",
  "version": "0.4.2",
  "description": "Hodor.",
  "repository": {
    "url": "https://github.com/mmkal/hodor"
  },
  "main": "dist/hodor/Hodor.js",
  "bin": "dist/cli.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p .",
    "test": "jest",
    "ci": "run-s clean build test",
    "release": "semantic-release",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "hodor"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "test/.*.Tests.ts$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "mmkal",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "coveralls": "^3.0.4",
    "decamelize": "^3.2.0",
    "jest": "^24.8.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "semantic-release": "^17.2.3",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "ava": "^2.0.0",
    "chalk": "^2.4.2"
  }
}
