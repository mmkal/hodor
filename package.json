{
  "name": "hodor",
  "version": "0.5.1",
  "description": "Hodor.",
  "repository": {
    "url": "https://github.com/mmkal/hodor"
  },
  "files": [
    "dist",
    "!dist/test"
  ],
  "main": "dist/hodor/Hodor.js",
  "bin": "dist/cli.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p .",
    "test": "jest",
    "coverage": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "keywords": [
    "hodor"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "test/.*.Tests.ts$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "author": "mmkal",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "14.14.31",
    "coveralls": "3.0.4",
    "jest": "24.8.0",
    "ts-jest": "24.0.2",
    "typescript": "4.7.4"
  }
}
